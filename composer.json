{
	"name": "shredio/core",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": ">=8.3",
		"marcj/topsort": "^2.0",
		"psr/simple-cache": "^3.0",
		"shredio/rapid-database-operations": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Shredio\\Core\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests"
		}
	},
	"scripts": {
		"phpstan": "phpstan analyse --memory-limit=1G",
		"test": "phpunit tests"
	},
	"require-dev": {
		"phpstan/phpstan": "^2.0.4",
		"symfony/serializer": "^7.1",
		"psr/http-message": "^2.0",
		"nyholm/psr7": "^1.8",
		"symfony/property-access": "^7.1",
		"symfony/validator": "^7.1",
		"ramsey/uuid": "^4.7",
		"paragonie/paseto": "^v3.2.3",
		"phpunit/phpunit": "^11.5",
		"nette/php-generator": "^4.1",
		"symfony/security-bundle": "^7.2",
		"symfony/cache": "^7.2",
		"symfony/framework-bundle": "^7.2",
		"shredio/voter": "^1.0",
		"symfony/psr-http-message-bridge": "^7.2",
		"runtime/swoole": "^0.4.0",
		"doctrine/doctrine-bundle": "^2.13",
		"doctrine/orm": "^3.3",
		"webchemistry/fixtures": "^2.0",
		"poliander/cron": "^3.2",
		"nette/neon": "^3.4",
		"symfony/browser-kit": "^7.2",
		"shredio/messenger": "^1.0",
		"symfony/messenger": "^7.2",
		"swoole/ide-helper": "^6.0",
		"dama/doctrine-test-bundle": "^8.3",
		"shredio/doctrine-queries": "^0.11.0"
	},
	"config": {
		"allow-plugins": {
			"symfony/runtime": true
		}
	}
}
