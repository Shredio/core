{
	"name": "shredio/core",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": ">=8.3"
	},
	"autoload": {
		"psr-4": {
			"Shredio\\Core\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests"
		}
	},
	"scripts": {
		"phpstan": "phpstan analyse",
		"test": "phpunit tests"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.12",
		"symfony/serializer": "^7.1",
		"psr/http-message": "^2.0",
		"nyholm/psr7": "^1.8",
		"symfony/property-access": "^7.1",
		"cycle/orm": "^2.9",
		"symfony/validator": "^7.1",
		"ramsey/uuid": "^4.7",
		"paragonie/paseto": "^v3.2.3",
		"cycle/annotated": "^4.2",
		"phpunit/phpunit": "^11.4",
		"nette/php-generator": "^4.1"
	}
}
